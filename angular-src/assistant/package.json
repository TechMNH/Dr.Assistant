{
  "name": "assistant",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:watch": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "serve": "ng serve --port 4200",
    "build": "npm run build:utility && npm run build:admin && npm run build:doctor && npm run build:patients && npm run build:signup && ng build",
    "build:watch": "concurrently \"npm run build:watch:utility\" \"npm run build:watch:admin\" \"npm run build:watch:doctor\" \"npm run build:watch:patients\" \"npm run build:watch:signup\" \"ng build --watch\"",
    "build:admin": "ng build admin",
    "build:doctor": "ng build doctor",
    "build:patients": "ng build patients",
    "build:signup": "ng build signup",
    "build:utility": "ng build utility",
    "start:admin": "npm run build:admin && npm run serve",
    "start:doctor": "npm run build:doctor && npm run serve",
    "start:patients": "npm run build:patients && npm run serve",
    "start:signup": "npm run build:signup && npm run serve",
    "start:utility": "npm run build:utility && npm run serve",
    "build:watch:admin": "ng build admin --watch",
    "build:watch:doctor": "ng build doctor --watch",
    "build:watch:patients": "ng build patients --watch",
    "build:watch:signup": "ng build signup --watch",
    "build:watch:utility": "ng build utility --watch",
    "start:watch:admin": "concurrently \"npm run build:watch:admin\" \"npm run serve\"",
    "start:watch:doctor": "concurrently \"npm run build:watch:doctor\" \"npm run serve\"",
    "start:watch:patients": "concurrently \"npm run build:watch:patients\" \"npm run serve\"",
    "start:watch:signup": "concurrently \"npm run build:watch:signup\" \"npm run serve\"",
    "start:watch:utility": "concurrently \"npm run build:watch:utility\" \"npm run serve\"",
    "deploy": "ng deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.1.0",
    "jquery": "^3.5.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "@angular-devkit/architect": "~0.900",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1"
  }
}
