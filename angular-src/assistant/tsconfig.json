{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "doctor": [
        "dist/doctor/doctor",
        "dist/doctor"
      ],
      "patients": [
        "dist/patients/patients",
        "dist/patients"
      ],
      "admin": [
        "dist/admin/admin",
        "dist/admin"
      ],
      "signup": [
        "dist/signup/signup",
        "dist/signup"
      ],
      "utility": [
        "dist/utility/utility",
        "dist/utility"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}